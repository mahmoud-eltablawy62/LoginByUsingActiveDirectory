
@model LoginDto 

<form method="post" asp-action="Login">
    <div class="form-group">
        <label for="exampleInputEmail1" asp-for="UserName" >UserName</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" asp-for="UserName">
    </div>
    <div class="form-group">
        <label for="exampleInputPassword1" asp-for="Password" >Password</label>
        <input type="password" class="form-control" id="exampleInputPassword1" asp-for="Password">
    </div>
   
    <button type="submit" class="btn btn-primary">Login</button>
</form>
